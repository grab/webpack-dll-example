{
  "name": "webpack-dll-example",
  "version": "0.0.1",
  "description": "Example for Webpack build optimizations",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "cross-env": "^3.0.0",
    "html-webpack-plugin": "^2.22.0",
    "react": "^15.3.2",
    "react-autobind": "^1.0.6",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.15.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config=webpack.config.dev.js --open",
    "build:dll": "cross-env NODE_ENV=development webpack --config=webpack.config.dll.js",
    "build": "cross-env NODE_ENV=production webpack --config=webpack.config.build.js",
    "postinstall": "npm run build:dll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grab/webpack-dll-example.git"
  },
  "keywords": [
    "webpack",
    "optimization",
    "dll",
    "build",
    "time",
    "performance"
  ],
  "author": "Grab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grab/webpack-dll-example/issues"
  },
  "homepage": "https://github.com/grab/webpack-dll-example#readme"
}
